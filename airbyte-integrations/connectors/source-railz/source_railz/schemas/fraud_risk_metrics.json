{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "reportId": {
      "type": ["string", "array"]
    },
    "startDate": {
      "type": "string",
      "format": "date-time"
    },
    "endDate": {
      "type": "string",
      "format": "date-time"
    },
    "businessName": {
      "type": "string"
    },
    "serviceName": {
      "type": "string"
    },
    "benfordLawScore": {
      "type": "number"
    },
    "outlierScore": {
      "type": ["null", "number"]
    },
    "outlierRatio": {
      "type": ["null", "number"]
    },
    "outlierTransactions": {
      "type": ["null", "array"],
      "items": {
        "type": ["null", "object"],
        "properties": {
          "id": {
            "type": "string"
          }
        }
      }
    },
    "transactionKeywords": {
      "type": ["null", "array"],
      "items": {
        "type": ["null", "object"],
        "properties": {
          "type": {
            "type": "string"
          },
          "count": {
            "type": "integer"
          }
        }
      }
    },
    "transactionsSectionStats": {
      "type": ["null", "array"],
      "items": {
        "type": ["null", "object"],
        "properties": {
          "section": {
            "type": "string"
          },
          "count": {
            "type": "integer"
          },
          "sdev": {
            "type": "number"
          }
        }
      }
    },
    "transactionPatterns": {
      "type": ["null", "array"],
      "items": {
        "type": ["null", "object"],
        "properties": {
          "repetitionPattern": {
            "type": "string"
          },
          "count": {
            "type": "integer"
          }
        }
      }
    },
    "currentRatioDelta": {
      "type": ["null", "number"]
    },
    "quickRatioDelta": {
      "type": ["null", "number"]
    },
    "debtToEquityRatioDelta": {
      "type": ["null", "number"]
    },
    "netProfitMarginRatioDelta": {
      "type": ["null", "number"]
    }
  }
}