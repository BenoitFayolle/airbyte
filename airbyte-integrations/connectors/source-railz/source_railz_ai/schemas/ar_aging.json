{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "reportId": {
      "type": ["string", "array"]
    },
    "startDate": {
      "type": "string",
      "format": "date-time"
    },
    "endDate": {
      "type": "string",
      "format": "date-time"
    },
    "businessName": {
      "type": "string"
    },
    "serviceName": {
      "type": "string"
    },
    "vendorRef": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": ["null", "string"]
        }
      }
    },
    "currencyRef": {
      "type": ["null", "object"],
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": ["null", "string"]
        },
        "symbol": {
          "type": ["null", "string"]
        }
      }
    },
    "current": {
      "type": ["null", "number"]
    },
    "dpd30": {
      "type": "number"
    },
    "dpd60": {
      "type": "number"
    },
    "dpd90": {
      "type": "number"
    },
    "dpd90+": {
      "type": ["null", "number"]
    },
    "total": {
      "type": "number"
    }
  }
}