{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": ["string", "integer"]
    },
    "reportId": {
      "type": ["string", "array"]
    },
    "startDate": {
      "type": "string",
      "format": "date-time"
    },
    "endDate": {
      "type": "string",
      "format": "date-time"
    },
    "businessName": {
      "type": "string"
    },
    "serviceName": {
      "type": "string"
    },
    "accountId": {
      "type": ["string", "integer"]
    },
    "date": {
      "type": "string",
      "format": "date-time"
    },
    "authorizedOnDate": {
      "type": ["null", "string"],
      "format": "date-time"
    },
    "isPending": {
      "type": "boolean"
    },
    "amount": {
      "type": "number"
    },
    "currency": {
      "type": "string"
    },
    "transactionType": {
      "type": ["null", "string"]
    },
    "paymentChannel": {
      "type": "string"
    },
    "categories": {
      "type": ["null", "string", "array"]
    },
    "merchantName": {
      "type": ["null", "string"]
    },
    "merchantStoreNumber": {
      "type": ["null", "string"]
    },
    "description": {
      "type": "string"
    },
    "location": {
      "type": ["null", "object"],
      "properties": {
        "line1": {
          "type": ["null", "string"]
        },
        "line2": {
          "type": ["null", "string"]
        },
        "city": {
          "type": ["null", "string"]
        },
        "region": {
          "type": ["null", "string"]
        },
        "country": {
          "type": ["null", "string"]
        },
        "postalCode": {
          "type": ["null", "string"]
        }
      }
    },
    "section": {
      "type": ["null", "string"]
    },
    "subSection": {
      "type": ["null", "string"]
    },
    "group": {
      "type": "string"
    },
    "subGroup": {
      "type": "string"
    }
  }
}